{
  "family": "frontend-app-task",
  "containerDefinitions": [
    {
      "name": "frontend-app-container",
      "image": "533267434977.dkr.ecr.us-east-2.amazonaws.com/hunlecb/frontend:latest",
      "essential": true,
      "portMappings": [
        {
          "containerPort": 3000,
          "hostPort": 80,
          "protocol": "tcp"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/frontend",
          "awslogs-region": "us-east-2",
          "awslogs-stream-prefix": "frontend"
        }
      }
    }
  ],
  "requiresCompatibilities": ["FARGATE"],
  "networkMode": "awsvpc",
  "cpu": "256",
  "memory": "512",
  "executionRoleArn": "arn:aws:iam::533267434977:role/ecsTaskExecutionRole"
}
