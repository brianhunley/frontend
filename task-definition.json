{
  "family": "frontend-app-task",
  "containerDefinitions": [
    {
      "name": "frontend-app-container",
      "image": "533267434977.dkr.ecr.us-east-2.amazonaws.com/hunlecb/frontend:latest",
      "essential": true,
      "portMappings": [
        {
          "containerPort": 80,
          "hostPort": 80,
          "protocol": "tcp"
        }
      ]
    }
  ],
  "requiresCompatibilities": ["FARGATE"],
  "networkMode": "awsvpc",
  "cpu": "256",
  "memory": "512",
  "executionRoleArn": "arn:aws:iam::533267434977:role/ecsTaskExecutionRole"
}
